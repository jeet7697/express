<%- include('../header.ejs') %>
<section class="login-wrap mtb-40">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-6">
                <div class="login-box">
                    <h1>Display data</h1>
                    <table class="table table-bordered text-black">
                        
                        <tr>
                            <th>No.</th>
                            <th>id</th>
                          <th>Name</th>
                          <th>Email</th>
                         
                        </tr>
                        <% var i=1 %>
                        
                            <!-- console.log(item) -->
                            <tr>
                                <td><%= i++ %></td>
                                <td><%= data._id %></td>
                                <td><%= data.name %></td>
                                <td><%= data.email %></td>
                               
                              </tr>
         
                       
                        
                      </table>
                </div>         
            </div>
        </div>
    </div>
</section>
<%- include('../footer.ejs') %>